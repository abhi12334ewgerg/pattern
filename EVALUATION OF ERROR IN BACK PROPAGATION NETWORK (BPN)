clc; 
clear all 
x1=0; 
x2=0; 
n=0.25; 
d=1; 
//s-1 
w11=0.6; 
w12=-0.3; 
w21=-0.1; 
w22=0.4; 
w1=0.4; 
w2=0.1; 
w01=0.3; 
w02=0.5; 
w0=-0.2; 
//s-2 
z1in=(x1*w11)+(x2*w21)+w01; z2in=(x2*w22)+(x1*w12)+w02; //s-3 
z1=1/(1+exp(-z1in)) 
z2=1/(1+exp(-z2in)) 
//s-4 
Yin=w0+(z1*w1)+(z2*w2) 
//s-5 
Y=1/(1+exp(-Yin)) 
//s-6 
del=Y*(1-Y)*(d-Y) 
//s-7 
delw1=n*del*z1 
delw2=n*del*z2 
delw0=n*del 
//s-8 
del11=z1*(1-z1)*delw1 
del22=z2*(1-z2)*delw2 
//s-9 
delw11=n*del11*x1 
delw12=n*del22*x1 
delw21=n*del11*x2 
delw22=n*del22*x2 
delw01=n*del11 
delw02=n*del22 
//s-10 
w0=w0+delw0 
w1=w1+delw1 
w2=w2+delw2 
w11=w11+delw11 
w12=w12+delw12 
w21=w21+delw21 
w22=w22+delw22 
w01=w01+delw01 
w02=w02+delw02 
z1in=(x1*w11)+(x2*w21)+w01; z2in=(x2*w22)+(x1*w12)+w02; z1=1/(1+exp(-z1in)) 
z2=1/(1+exp(-z2in)) 
Yin=w0+(z1*w1)+(z2*w2) 
Y=1/(1+exp(-Yin)) 
err=d-Y 
disp("w1",w1) 
disp("w2",w2) 
disp("w0",w0) 
disp("w11",w11) 
disp("w12",w12) 
disp("w21",w21) 
disp("w22",w22) 
disp("w01",w01) 
disp("w02",w02) 
disp("Error is ",err) 
